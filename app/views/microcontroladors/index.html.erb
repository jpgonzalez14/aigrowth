<p id="notice"><%= notice %></p>

<h1>Microcontroladors</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @microcontroladors.each do |microcontrolador| %>
      <tr>
        <td><%= microcontrolador.nombre %></td>
        <td><%= link_to 'Show', microcontrolador %></td>
        <td><%= link_to 'Edit', edit_microcontrolador_path(microcontrolador) %></td>
        <td><%= link_to 'Destroy', microcontrolador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Microcontrolador', new_microcontrolador_path %>
